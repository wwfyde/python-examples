   },
   "cell_type": "code",
   "source": [
    "# 反转字符串\n",
    "s = \"somestring\"\n",
    "l = [i for i in s]\n",
    "times = len(s) // 2\n",
    "for i in range(times):\n",
    "    l[i], l[-i - 1] = l[-i - 1], l[i]\n",
    "print(l)\n"
   ],
   "id": "459e0624f8cfa9d4",
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "['g', 'n', 'i', 'r', 't', 's', 'e', 'm', 'o', 's']\n"
     ]
    }
   ],
   "execution_count": 10
  },
  {
   "metadata": {
